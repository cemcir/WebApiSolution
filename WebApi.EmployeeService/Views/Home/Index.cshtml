@model WebApi.EmployeeService.Models.EmployeeListModel

<table class="table">
    <thead>
        <tr>
            <th scope="col">Numara</th>
            <th scope="col">Ad</th>
            <th scope="col">Soyad</th>
            <th scope="col">Maaş</th>
            <th scope="col">Cinsiyet</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.Employees) {
            <tr>
                <th scope="row">@(Model.Count++)</th>
                <td>@item.FirstName</td>
                <td>@item.LastName</td>
                <td>@item.Salary</td>
                <td>@item.Gender</td>
            </tr>
        }
    </tbody>
</table>
